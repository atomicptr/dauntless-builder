{
	"name": "dauntless-builder",
	"version": "3.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run check && vite build && rm -rf build && cp -r .svelte-kit/cloudflare build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "npx -y @biomejs/biome format src *.ts *.js --write",
		"test": "vitest run"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@noble/hashes": "^1.6.1",
		"@sveltejs/adapter-cloudflare": "^4.9.0",
		"@sveltejs/kit": "^2.15.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"@types/showdown": "^2.0.6",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.22",
		"neverthrow": "^8.1.1",
		"postcss": "^8.4.49",
		"showdown": "^2.1.0",
		"sqids": "^0.3.0",
		"svelte": "^5.16.0",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^3.4.17",
		"ts-pattern": "^5.6.0",
		"typescript": "^5.7.2",
		"vite": "^5.4.11",
		"vitest": "^2.1.8"
	},
	"type": "module"
}